class cat():
    def __init__(self,name):
        self.enrg = 1000
        self.name = name
    def setName(self, name):
        self.name = name
    def gatName(Name):
        return Name
    def run(self,enrg):
        self.enrg = enrg - 100
    def getStamina():
        return enrg
    def eat(self,enrg):
        self.enrg += 200
    def sleep(self,enrg):
        self.enrg += 100
