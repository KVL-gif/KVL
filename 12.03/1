class cat():
    def __init__(self):
        self.enrg = 1000
    def setName(self,name):
        self.name = name
    def gatName(self):
        return self.name
    def run(self):
        self.enrg = self.enrg - 100
    def getStamina(self):
        return self.enrg
    def eat(self):
        self.enrg += 200
    def sleep(self):
        self.enrg += 100
