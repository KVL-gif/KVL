class cat():
    def __init__(self,enrg):
        self.enrg = 1000
    def setName(self, Name):
        self.Name = Name
    def gatName(Name):
        return Name
    def run():
        enrg = enrg - 100
    def getStamina():
        return enrg
    def eat():
        enrg += 200
    def sleep():
        enrg += 100
